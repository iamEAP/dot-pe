      &lt;picture&gt;
        &lt;source
          srcset=&quot;/terson/static/d9642c0dddbbbcfb3ffa573e4f1444a0/53e9f/finished-product.webp 340w,
/terson/static/d9642c0dddbbbcfb3ffa573e4f1444a0/0991f/finished-product.webp 680w,
/terson/static/d9642c0dddbbbcfb3ffa573e4f1444a0/ecaf8/finished-product.webp 1000w&quot;
